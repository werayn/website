{
    "plugins": [
        {
            "name": "esdoc-standard-plugin",
            "title": "Junique Virgile Website",
            "option": {
              "undocumentIdentifier": { "enable": false },
              "unexportedIdentifier": { "enable": true },
              "lint": {"enable": true},
              "typeInference": {"enable": true},
                "coverage": {
                    "enable": true
                  },
                  "accessor": {
                    "access": ["public", "protected", "private"],
                    "autoPrivate": false
                  },
                "manual": {
                  "index": "./manual/index.md",
                  "asset": "./src/assets/logo.png",
                  "globalIndex": true,
                  "files": [
                    "./manual/configuration.md",
                    "./manual/installation.md",
                    "./manual/architecture.md",
                    "./manual/tutorial.md",
                    "./manual/how-to-doc.md",
                    "./manual/new-feature.md",
                    "./README.md"
                  ]
                },
                "experimentalProposal": {
                    "classProperties": true,
                    "objectRestSpread": true
                }
            }
        },
        {
            "name": "esdoc-brand-plugin",
            "option": {
              "title": "Junique Virgile",
              "description": "Junique Virgile Website",
              "repository": "https://github.com/werayn/website",
              "site": "www.junique-virgile.fr",
              "author": "https://github.com/werayn"
            }
        },
        {
             "name": "esdoc-react-plugin"
        },
        {
            "name": "esdoc-undocumented-identifier-plugin",
            "option": {
                    "enable": true
                }
        },
        {
          "name": "esdoc-ecmascript-proposal-plugin",
          "option": {
            "classProperties": true,
            "objectRestSpread": true,
            "doExpressions": true,
            "functionBind": true,
            "functionSent": true,
            "asyncGenerators": true,
            "decorators": true,
            "exportExtensions": true,
            "dynamicImport": true
          }
        },
        {
            "name": "esdoc-jsx-plugin",
            "option": {"enable": true}
        }
    ],
    "source": "./src",
    "destination": "./doc",
    "includes": ["\\.(js|jsx)$"],
    "package": "./package.json",
    "index": "./README.MD"
}
