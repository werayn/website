{
  "name": "Seedz-website",
  "version": "1.0.0",
  "description": "Seedz Website",
  "main": "index.js",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "bulma": "^0.7.1",
    "bulma-extensions": "^2.2.1",
    "fingerprintjs2": "^1.8.0",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "lodash": "^4.17.10",
    "node-sass": "^4.9.1",
    "normalize.css": "^8.0.0",
    "normalizr": "^3.2.4",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.1",
    "proptypes": "^1.1.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-i18nify": "^1.11.14",
    "react-loader-spinner": "^2.1.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "react-transition-group": "^2.4.0",
    "react-verification-input": "^0.1.3",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "redux-persist-transform-immutable": "^5.0.0",
    "redux-promise-middleware": "^5.1.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "serve": "^9.4.2",
    "standard-http-error": "^2.0.1",
    "ua-parser-js": "^0.7.18",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "build-js": "react-scripts build",
    "build-css": "node-sass --include-path ./node_modules src/styles/main.scss -o src/styles/",
    "watch-css": "npm run build-css && node-sass --include-path ./node_modules src/styles/main.scss -o src/styles/ --watch main.scss:node_modules/bulma/css/bulma.css --recursive",
    "serve": "serve -n -l 3000 -s build",
    "build": "npm-run-all build-css build-js",
    "start-js": "react-scripts start",
    "start:dev": "npm-run-all -p watch-css start-js",
    "start:prod": "npm-run-all build serve",
    "test-JS": "react-scripts test --env=jsdom",
    "test": "npm-run-all -p watch-css test-JS",
    "eject": "react-scripts eject",
    "lint": "./node_modules/.bin/eslint \"src/**/*.js*\"",
    "serve-doc": "serve -d -n -l 3001 doc",
    "gen-doc": "esdoc -c ./.esdoc.json",
    "doc": "npm-run-all gen-doc serve-doc"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "esdoc": "^1.1.0",
    "esdoc-brand-plugin": "^1.0.1",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-jsx-plugin": "^1.0.0",
    "esdoc-publish-html-plugin": "^1.1.2",
    "esdoc-react-plugin": "^1.0.1",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.19.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-eslint-comments": "^2.0.2",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "redux-devtools-extension": "^2.13.2"
  }
}
